@{
}
<div class="text-center">
<h1>Travel to @Model.Name </h1>
<table class="table">
    <tr>
        <th>Id</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Start Date</th>
        <td>@Model.Start_Date</td>
    </tr>
    <tr>
        <th>End Date</th>
        <td>@Model.End_Date</td>
    </tr>
    <tr>
        <th>Start Place</th>
        <td>@Model.Start_Place</td>
    </tr>
    <tr>
        <th>End Place</th>
        <td>@Model.End_Place</td>
    </tr>
    <tr>
        <th>Participants</th>
        <td>@Model.Participants</td>
    </tr>
    <tr>
        <th>Guide</th>
        <td>@Model.Guide</td>
    </tr>
</table>
    <a asp-action="Index" class="btn btn-primary" style="padding-top : 5px">Go Back</a>
</div>